{
  "name": "n8n-nodes-spamassassin-mcp",
  "version": "1.1.3",
  "description": "n8n community node for SpamAssassin MCP server integration",
  "keywords": [
    "n8n-community-node-package",
    "spamassassin",
    "mcp",
    "email",
    "spam",
    "security"
  ],
  "license": "MIT",
  "homepage": "https://github.com/btafoya/spamassassin-mcp-n8n-community-node",
  "author": {
    "name": "Brian Tafoya",
    "email": "btafoya@briantafoya.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btafoya/spamassassin-mcp-n8n-community-node.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "cp nodes/SpamAssassinMcp/spamassassin.svg dist/nodes/SpamAssassinMcp/",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint **/*.ts",
    "lintfix": "eslint nodes credentials --fix",
    "prepublishOnly": "npm run build && npm run lint -s",
    "release": "bash scripts/release.sh",
    "release:minor": "bash scripts/release.sh minor",
    "release:major": "bash scripts/release.sh major"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SpamAssassinMcpApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/SpamAssassinMcp/SpamAssassinMcp.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0"
  }
}
